{
  "name": "JIRAfa",
  "description": "Chrome Extension project altering Atlassian JIRA.",
  "version": "0.9",
  "manifest_version": 2,
  "permissions": [
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "3rd-party/jquery-3.2.1.min.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "main.js",
    "modules/jira.js",
    "modules/utils.js",
    "modules/backlog.js"
  ]
}